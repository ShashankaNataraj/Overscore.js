{
  "name": "overcore.js",
  "version": "0.0.1",
  "description": "UnderscoreJS implemented with ES6",
  "main": "src/index.js",
  "scripts": {
    "babel": "babel-node src/index.js --presets es2015,stage-2",
    "test": "babel-node spec/run.js && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShashankaNataraj/Overscore.js.git"
  },
  "keywords": [
    "Underscore",
    "lodash",
    "clone",
    "utility"
  ],
  "author": "Shashanka Nataraj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShashankaNataraj/Overscore.js/issues"
  },
  "homepage": "https://github.com/ShashankaNataraj/Overscore.js#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^4.1.1"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "**/*.{js}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}